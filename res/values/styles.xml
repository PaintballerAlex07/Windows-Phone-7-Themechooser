<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <style name="Windows Phone 7" parent="@android:style/Theme" />
    <style name="DialtactsTheme" parent="@android:style/Theme.Wallpaper">
        <item name="android:windowBackground">@drawable/colourblack</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="SuggestionQueryTextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="SuggestionSuggestedTextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="StatusBar.ClearButton" parent="@android:style/Widget.Button">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Theme" parent="@android:style/Theme">
        <item name="android:colorBackground">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowShowWallpaper">true</item>
    </style>
    <style name="Theme.InputMethod" parent="@android:style/Theme.Panel">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.InputMethod</item>
        <item name="android:imeFullscreenBackground">@android:drawable/input_method_fullscreen_background</item>
        <item name="android:imeExtractEnterAnimation">@android:anim/input_method_extract_enter</item>
        <item name="android:imeExtractExitAnimation">@android:anim/input_method_extract_exit</item>
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="TextAppearance" parent="@android:style/TextAppearance">
        <item name="android:textColorHighlight">#ff137dab</item>
        <item name="android:textColorLink">#ff19a2de</item>
    </style>
    <style name="TextAppearance.StatusBar" parent="@android:style/TextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TextAppearance.StatusBar.CarrierLabel" parent="@android:style/TextAppearance.Large">
        <item name="android:textColor">@color/systemui_carrier_label_text_color</item>
    </style>
    <style name="TextAppearance.StatusBar.EventContent" parent="@style/TextAppearance.StatusBar">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TextAppearance.StatusBar.EventContent.Title" parent="@style/TextAppearance.StatusBar.EventContent">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TextAppearance.StatusBar.Icon" parent="@android:style/Widget.RatingBar.Indicator">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TextAppearance.StatusBar.Title" parent="@android:style/Widget.RatingBar.Indicator">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Widget.Button" parent="@android:style/Widget">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/frameworks_res_btn_default</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="Widget.CompoundButton" parent="@android:style/Widget">
        <item name="android:textColor">#ffffffff</item>
        <item name="gravity">left|center</item>
        <item name="focusable">true</item>
        <item name="clickable">true</item>
    </style>
    <style name="Widget.ProgressBar.Horizontal" parent="@android:style/Widget.ProgressBar">
        <item name="android:maxHeight">20.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/frameworks_res_progress_indeterminate_horizontal</item>
        <item name="android:progressDrawable">@drawable/frameworks_res_progress_horizontal</item>
        <item name="android:minHeight">20.0dip</item>
        <item name="maxHeight">20.0dip</item>
        <item name="indeterminateOnly">false</item>
        <item name="indeterminateDrawable">@drawable/frameworks_res_progress_indeterminate_horizontal</item>
        <item name="progressDrawable">@drawable/frameworks_res_progress_horizontal</item>
        <item name="minHeight">20.0dip</item>
    </style>
    <style name="Widget.SeekBar" parent="@android:style/Widget">
        <item name="android:focusable">true</item>
        <item name="android:maxHeight">20.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/frameworks_res_progress_indeterminate_horizontal</item>
        <item name="android:progressDrawable">@drawable/frameworks_res_progress_horizontal</item>
        <item name="android:minHeight">20.0dip</item>
        <item name="android:thumb">@drawable/frameworks_res_seek_thumb</item>
        <item name="android:thumbOffset">8.0dip</item>
    </style>
</resources>